@model IEnumerable<AAOAdmin.Models.DriversAvailable>

@{
    ViewData["Title"] = "test";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>test</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
      <tr>
        <th>
          @Html.DisplayNameFor(model => model.DriversAvailableDate)
        </th>
        <th>
          @Html.DisplayNameFor(model => model.User.UserPhoneNumber)
        </th>
        <th>
          @Html.DisplayNameFor(model => model.User.UserFullName)
        </th>
        <th></th>
      </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
      <tr>
        <td>
